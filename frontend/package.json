{
  "name": "PhillyReads",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "2.0.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "bootstrap": "^4.1.3",
    "husky": "^1.1.0",
    "i": "^0.3.6",
    "lint-staged": "^7.3.0",
    "memoize-one": "^4.0.3",
    "node-sass": "^4.9.3",
    "npm": "^6.4.1",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.8.6",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-dropzone": "^7.0.1",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.5.0",
    "universal-cookie": "^3.0.4",
    "whatwg-fetch": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "format": "prettier --single-quote --write '{src,test}/**/*.{js,jsx,json,scss}'",
    "format:check": "prettier --single-quote --list-different \"{src,test}/**/*.js\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
